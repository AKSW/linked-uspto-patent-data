							<ipcEntry kind="3" symbol="C12H0001080000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
								<textBody><title>
										<titlePart><text>by heating</text>
										</titlePart>
									</title>
								</textBody>
							</ipcEntry>
						</ipcEntry>
						<ipcEntry kind="2" symbol="C12H0001100000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
							<textBody><title>
									<titlePart><text>Precipitation by chemical means</text>
									</titlePart>
								</title>
							</textBody>
						</ipcEntry>
					</ipcEntry>
					<ipcEntry kind="1" symbol="C12H0001120000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>without precipitation</text>
								</titlePart>
							</title>
						</textBody>
						<ipcEntry kind="2" symbol="C12H0001140000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
							<textBody><title>
									<titlePart><text>with non-precipitating compounds, e.g. sulphiting</text>
									</titlePart>
									<titlePart><text>Sequestration, e.g. with chelate-producing compounds</text>
									</titlePart>
								</title>
							</textBody>
						</ipcEntry>
						<ipcEntry kind="2" symbol="C12H0001160000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
							<textBody><title>
									<titlePart><text>by physical means, e.g. irradiation</text>
									</titlePart>
								</title>
							</textBody>
							<ipcEntry kind="3" symbol="C12H0001180000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
								<textBody><title>
										<titlePart><text>by heating</text>
										</titlePart>
									</title>
								</textBody>
								<ipcEntry kind="4" symbol="C12H0001200000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
									<textBody><title>
											<titlePart><text>in containers allowing for expansion of the contents</text>
											</titlePart>
										</title>
									</textBody>
								</ipcEntry>
							</ipcEntry>
						</ipcEntry>
					</ipcEntry>
					<ipcEntry kind="1" symbol="C12H0001220000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>Ageing or ripening by storing, e.g. lagering of beer</text>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
				</ipcEntry>
			</ipcEntry>
		</ipcEntry>
	</ipcEntry>
</ipcEntries>
