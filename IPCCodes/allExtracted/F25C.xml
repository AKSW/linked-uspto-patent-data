								<ipcEntry kind="4" symbol="F25C0005100000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
									<textBody><title>
											<titlePart><text>using hot refrigerant</text>
											</titlePart>
											<titlePart><text>using fluid heated by refrigerant</text>
											</titlePart>
										</title>
									</textBody>
								</ipcEntry>
							</ipcEntry>
							<ipcEntry kind="3" symbol="F25C0005120000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
