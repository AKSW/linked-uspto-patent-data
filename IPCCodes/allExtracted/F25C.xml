					<textBody><title>
							<titlePart><text>Production of ice or snow for the purposes specified in the sub-groups of this group</text>
							</titlePart>
						</title>
					</textBody>
					<ipcEntry kind="1" symbol="F25C0003020000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>for ice rinks</text>
									<entryReference>shape of rinks <sref ref="A63C0019100000" refType="/" refLevel="c"></sref>
									</entryReference>
									<entryReference>construction of rinks <sref ref="E01C" refType="/" refLevel="c"></sref>
									</entryReference>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
					<ipcEntry kind="1" symbol="F25C0003040000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
