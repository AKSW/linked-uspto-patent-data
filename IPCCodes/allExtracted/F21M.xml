						<ipcEntry kind="2" symbol="F21M0003180000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
							<textBody><title>
									<titlePart><text>with provision for changing shape or direction of the beam</text>
