		<ipcEntry kind="c" symbol="F23" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
			<textBody><title>
					<titlePart><text>COMBUSTION APPARATUS</text>
