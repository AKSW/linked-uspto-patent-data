						<ipcEntry kind="2" symbol="F24F0003040000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
							<textBody><title>
									<titlePart><text>operating with high pressure or high velocity</text>
