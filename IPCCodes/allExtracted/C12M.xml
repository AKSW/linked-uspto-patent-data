				<ipcEntry edition="19800101" kind="m" symbol="C12M0001000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Apparatus for enzymology or microbiology</text>
