	<ipcEntry kind="s" symbol="H" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
		<textBody><title>
				<titlePart><text>SECTION H <emdash></emdash> ELECTRICITY</text>
				</titlePart>
			</title>
		</textBody>
		<ipcEntry kind="c" symbol="H01" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
			<textBody><title>
					<titlePart><text>BASIC ELECTRIC ELEMENTS</text>
					</titlePart>
				</title>
			</textBody>
			<ipcEntry kind="n" symbol="H01" entryType="K" ipcLevel="C" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
				<textBody>
					<note type="none">
						<noteParagraph><text>Processes involving only a single technical art, e.g. drying, coating, for which provision exists elsewhere are classified in the relevant class for that art.</text>
						</noteParagraph>
					</note>
				</textBody>
			</ipcEntry>
			<ipcEntry kind="u" symbol="H01C" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
				<textBody><title>
						<titlePart><text>RESISTORS</text>
						</titlePart>
					</title>
				</textBody>
				<ipcEntry kind="n" symbol="H01C" entryType="K" ipcLevel="C" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody>
						<note type="number">
							<noteParagraph edition="19740701"><text>In this sub-class, the term "adjustable" means mechanically adjustable.</text>
							</noteParagraph>
							<noteParagraph edition="19740701"><text>Variable resistors, the value of which is changed non-mechanically, e.g. by voltage or temperature, are classified in group <sref ref="H01C0007000000" refType="/" refLevel="c"/>.</text>
							</noteParagraph>
						</note>
					</textBody>
				</ipcEntry>
				<ipcEntry kind="m" symbol="H01C0001000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Details</text>
							</titlePart>
						</title>
					</textBody>
