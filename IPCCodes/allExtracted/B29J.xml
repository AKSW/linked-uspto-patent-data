	<ipcEntry kind="s" symbol="B" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
		<textBody><title>
				<titlePart><text>SECTION B <emdash></emdash> PERFORMING OPERATIONS</text>
				</titlePart>
				<titlePart><text>TRANSPORTING</text>
				</titlePart>
			</title>
		</textBody>
		<ipcEntry kind="t" symbol="B21" endSymbol="B21" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
			<textBody><title>
					<titlePart><text>SHAPING</text>
					</titlePart>
				</title>
			</textBody>
		</ipcEntry>
		<ipcEntry kind="c" symbol="B29" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
			<textBody><title>
					<titlePart><text>WORKING OF PLASTICS</text>
					</titlePart>
					<titlePart><text>WORKING OF SUBSTANCES IN A PLASTIC STATE, IN GENERAL</text>
					</titlePart>
					<titlePart><text>WORKING OF SUBSTANCES NOT OTHERWISE PROVIDED FOR</text>
						<entryReference>see Section C, particularly <sref ref="C08" refType="/" refLevel="c"></sref>, for chemical matters</entryReference>
						<entryReference>shaping clay or other ceramic compositions, slag, or mixtures containing cementitious material <sref ref="B28B" refType="/" refLevel="c"></sref>
						</entryReference>
					</titlePart>
				</title>
			</textBody>
			<ipcEntry kind="n" symbol="B29" entryType="K" ipcLevel="C" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
				<textBody>
					<note type="number">
						<noteParagraph><text>For the purpose of this class, "plastics" means macromolecular compounds or compositions based thereon.</text>
						</noteParagraph>
						<noteParagraph><text>A product is not classified here unless it results from a process which is itself classified in this class.</text>
						</noteParagraph>
					</note>
				</textBody>
			</ipcEntry>
			<ipcEntry kind="u" symbol="B29J" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
				<textBody><title>
						<titlePart><text>SHAPING OR WORKING OF MATERIALS, NOT OTHERWISE PROVIDED FOR</text>
						</titlePart>
						<titlePart><text>MOULDING BOARDS OR ARTICLES FROM WOOD PARTICLES</text>
						</titlePart>
					</title>
				</textBody>
				<ipcEntry kind="n" symbol="B29J" entryType="K" ipcLevel="C" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody>
						<note type="none">
							<noteParagraph><text>This sub-class applies only if the main constituent of the material is not covered by any other sub-class.</text>
							</noteParagraph>
						</note>
					</textBody>
				</ipcEntry>
				<ipcEntry kind="m" symbol="B29J0001000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Shaping or working of materials, not otherwise provided for</text>
							</titlePart>
						</title>
					</textBody>
					<ipcEntry kind="1" symbol="B29J0001020000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>containing large proportions of fibrous fillers</text>
									<entryReference><sref ref="B29J0005000000" refType="/" refLevel="c"></sref> takes precedence</entryReference>
								</titlePart>
							</title>
						</textBody>
						<ipcEntry kind="2" symbol="B29J0001040000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
							<textBody><title>
									<titlePart><text>Making leather substitutes, e.g. with leather fillers</text>
										<entryReference>consisting of a fibrous web coated with a layer of macromolecular material <sref ref="D06N" refType="/" refLevel="c"></sref>
										</entryReference>
									</titlePart>
								</title>
							</textBody>
						</ipcEntry>
					</ipcEntry>
				</ipcEntry>
				<ipcEntry kind="m" symbol="B29J0005000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Making particle board, fibre board, or articles moulded from wood particles or fibres, with or without added binding agents</text>
								<entryReference>with water-setting binding agents <sref ref="B28B" refType="/" refLevel="c"></sref>
								</entryReference>
								<entryReference>apparatus applicable to plastic materials in general <sref ref="B29C" refType="/" refLevel="c"></sref>, <sref ref="B29F" refType="/" refLevel="c"></sref>
								</entryReference>
								<entryReference>composition of the material used, <u>see</u> the relevant chemical class, particularly <sref ref="C08L0097020000" refType="/" refLevel="c"></sref>
								</entryReference>
								<entryReference>in the manner of paper-making <sref ref="D21J" refType="/" refLevel="c"></sref>
								</entryReference>
							</titlePart>
						</title>
					</textBody>
					<ipcEntry kind="1" symbol="B29J0005020000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>Mixing wood particles with binding agents</text>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
					<ipcEntry kind="1" symbol="B29J0005040000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>Moulding and subsequent pressing to produce boards or shaped mats</text>
									<entryReference><sref ref="B29J0005080000" refType="/" refLevel="c"></sref> takes precedence</entryReference>
								</titlePart>
								<titlePart><text>Presses specially designed therefor</text>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
					<ipcEntry kind="1" symbol="B29J0005060000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>Moulding and subsequent pressing to produce hollow articles, e.g. vessels, casings</text>
								</titlePart>
								<titlePart><text>Presses specially designed therefor</text>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
					<ipcEntry kind="1" symbol="B29J0005080000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>Plants for working continuously</text>
								</titlePart>
								<titlePart><text>Presses specially designed therefor</text>
								</titlePart>
							</title>
						</textBody>
						<ipcEntry kind="2" symbol="B29J0005100000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
							<textBody><title>
									<titlePart><text>in which the compressing means move with the material</text>
									</titlePart>
									<titlePart><text>Accessories therefor</text>
									</titlePart>
								</title>
							</textBody>
						</ipcEntry>
					</ipcEntry>
				</ipcEntry>
			</ipcEntry>
		</ipcEntry>
	</ipcEntry>
</ipcEntries>
