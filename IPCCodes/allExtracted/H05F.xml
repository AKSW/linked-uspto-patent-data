<ipcEntries displayMode="e19800101;a19800101;c19800101" ipcLevel="c" split="subclass" languages=",en,fr," size="8074">
	<ipcEntry kind="s" symbol="H" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
		<textBody><title>
				<titlePart><text>SECTION H <emdash></emdash> ELECTRICITY</text>
				</titlePart>
			</title>
		</textBody>
		<ipcEntry kind="c" symbol="H05" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
			<textBody><title>
					<titlePart><text>ELECTRIC TECHNIQUES NOT OTHERWISE PROVIDED FOR</text>
					</titlePart>
				</title>
			</textBody>
			<ipcEntry kind="u" symbol="H05F" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
				<textBody><title>
						<titlePart><text>STATIC ELECTRICITY</text>
						</titlePart>
						<titlePart><text>NATURALLY-OCCURRING ELECTRICITY</text>
							<entryReference>electrostatic machines <sref ref="H02N" refType="/" refLevel="c"></sref>
							</entryReference>
							<entryReference>uses of electricity in performing operations, e.g. precipitation, <u>see</u> the relevant sub-classes for the operations</entryReference>
						</titlePart>
					</title>
				</textBody>
				<ipcEntry kind="n" symbol="H05F" entryType="K" ipcLevel="C" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody>
						<note type="none">
							<noteParagraph><text>This sub-class deals with methods or arrangements for preventing the formation of electrostatic charges on bodies or for carrying-off these charges after their formation. Specific applications of these methods or arrangements, e.g. during the manufacture of artificial fibres or films, are dealt with in the relevant sub-classes.</text>
							</noteParagraph>
						</note>
					</textBody>
				</ipcEntry>
				<ipcEntry kind="m" symbol="H05F0001000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Preventing the formation of electrostatic charges</text>
							</titlePart>
						</title>
					</textBody>
					<ipcEntry kind="1" symbol="H05F0001020000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>by surface treatment</text>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
				</ipcEntry>
				<ipcEntry kind="m" symbol="H05F0003000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Carrying-off electrostatic charges</text>
								<entryReference>from living beings <sref ref="A61N0001140000" refType="/" refLevel="c"></sref>
								</entryReference>
							</titlePart>
						</title>
					</textBody>
					<ipcEntry kind="1" symbol="H05F0003020000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>by means of earthing connections</text>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
					<ipcEntry edition="19740701" kind="1" symbol="H05F0003040000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>by means of spark gaps or other discharge devices</text>
									<entryReference>devices providing for corona discharge <u>per se</u>
										<sref ref="H01T0019000000" refType="/" refLevel="c"></sref>
									</entryReference>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
					<ipcEntry kind="1" symbol="H05F0003060000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>by means of ionising radiation</text>
								</titlePart>
							</title>
						</textBody>
					</ipcEntry>
				</ipcEntry>
				<ipcEntry kind="m" symbol="H05F0007000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Using naturally-occurring electricity</text>
							</titlePart>
						</title>
					</textBody>
				</ipcEntry>
			</ipcEntry>
		</ipcEntry>
	</ipcEntry>
</ipcEntries>
