				<ipcEntry kind="m" symbol="C05D0003000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Calcareous fertilisers</text>
