					<ipcEntry kind="1" symbol="C05D0003020000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
						<textBody><title>
								<titlePart><text>from limestone, calcium carbonate, calcium hydrate, slaked lime, calcium oxide, waste calcium compounds</text>
								</titlePart>
							</title>
						</textBody>
