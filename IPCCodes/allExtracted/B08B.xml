						<ipcEntry kind="2" symbol="B08B0003080000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
							<textBody><title>
									<titlePart><text>the liquid having chemical or dissolving effect</text>
										<entryReference>substances used <u>per se</u>, <u>see</u> the relevant classes</entryReference>
									</titlePart>
								</title>
							</textBody>
