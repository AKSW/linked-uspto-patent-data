				<ipcEntry kind="m" symbol="D04H0005000000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
					<textBody><title>
							<titlePart><text>Non-woven fabrics formed of mixtures of relatively-short fibres and yarns or like filamentary material of substantial length</text>
							</titlePart>
						</title>
					</textBody>
					<ipcEntry kind="1" symbol="D04H0005020000" ipcLevel="C" entryType="K" isRenumbered="N" isDead="N" lang="EN" noadvanced="yes">
